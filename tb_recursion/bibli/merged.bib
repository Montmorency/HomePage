@Article{	  haydockheine75,
  author	= {R Haydock and V Heine and M J Kelly},
  title		= {Electronic structure based on the local atomic environment
		  for tight-binding bands. II},
  journal	= {Journal of Physics C: Solid State Physics},
  volume	= {8},
  number	= {16},
  pages		= {2591},
  url		= {http://stacks.iop.org/0022-3719/8/i=16/a=011},
  year		= {1975},
  abstract	= {For pt.I see abstr. A80956 of 1972. A number of methods
		  previously used for calculating the local electronic
		  structure for tight-binding bands were based on the local
		  atomic environment without the use of Bloch's theorem. In
		  applying the recursion method to a variety of problems, the
		  authors have simplified, generalized and refined their
		  original ideas. The authors (i) present a more definitive
		  statement of the recursion method (ii) gather together a
		  number of practical points arising from the computation of
		  the continued-fraction expansion of the Green function and
		  (iii) relate the techniques to other approaches to the same
		  problems.}
}

@Article{	  haydockkelly75,
  author	= {R Haydock and M J Kelly},
  title		= {Electronic structure from non-hermitian representations of
		  the Hamiltonian},
  journal	= {Journal of Physics C: Solid State Physics},
  volume	= {8},
  number	= {13},
  pages		= {L290},
  url		= {http://stacks.iop.org/0022-3719/8/i=13/a=003},
  year		= {1975},
  abstract	= {The recursion method, used so far for the tight-binding
		  Hamiltonian, implicitly requires the representation to be
		  hermitian. Non-hermitian representations arise in
		  (chemical) pseudopotential theory and the authors indicate
		  how to obtain the local density of states in such a more
		  general case. Results are given for the electronic
		  structure of crystalline arsenic.}
}

@Article{	  haydocklaves75,
  author	= {R Haydock and R L Johannes},
  title		= {The electronic structure of transition metal Laves
		  phases},
  journal	= {Journal of Physics F: Metal Physics},
  volume	= {5},
  number	= {11},
  pages		= {2055},
  url		= {http://stacks.iop.org/0305-4608/5/i=11/a=017},
  year		= {1975},
  abstract	= {The problem of understanding AB 2 transition metal alloy
		  structural trends within the three Laves phases is
		  approached by considering a common-band model alloy.
		  Tight-binding d band densities of states are calculated for
		  the three phases by the recursion method of Haydock, Heine
		  and Kelly (1972). The three total densities of states are
		  very similar, highly peaked structures. The A site local
		  densities of states show a division into bonding and
		  antibonding states. Cohesive energies are computed and it
		  is found that their differences undergo a cyclic variation
		  across the band, which is related to the moments of the
		  densities of states, and hence to the layer stacking of the
		  structures.}
}

