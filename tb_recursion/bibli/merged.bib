@article{haydock84,
  author	= {R Haydock and C M M Nex},
  title		= {Comparison of quadrature and termination for estimating
		  the density of states within the recursion method},
  journal	= {Journal of Physics C: Solid State Physics},
  volume	= {17},
  number	= {27},
  pages		= {4783},
  url		= {http://stacks.iop.org/0022-3719/17/i=27/a=008},
  year		= {1984},
  abstract	= {Within the recursion method, the local density of states
		  is estimated from a truncated continued fraction (Jacobi
		  matrix). For simple one- and two-band models, the authors
		  compare the two methods for calculating the density of
		  states. The quadrature method assumes no information other
		  than the truncated fraction. Given exact band edges in
		  addition to the truncated fraction, an analytic terminator
		  can do better than quadrature if the internal singularities
		  are sufficiently weak.}
}

@Article{	  haydock85,
  author	= {R Haydock and C M M Nex},
  title		= {A general terminator for the recursion method},
  journal	= {Journal of Physics C: Solid State Physics},
  volume	= {18},
  number	= {11},
  pages		= {2235},
  url		= {http://stacks.iop.org/0022-3719/18/i=11/a=007},
  year		= {1985},
  abstract	= {A general method is presented for approximating continuous
		  densities of states, and wavefunctions over infinite
		  regions, within the recursion method by means of a
		  terminator which is an infinite set of approximate basis
		  states and their Hamiltonian matrix elements. This is an
		  analytical and computational solution to the problem of
		  approximating the projected density of states for a
		  Hamiltonian having an arbitrary number of bands with van
		  Hove singularities. It also enables calculation of charge
		  densities and other quantities related to the
		  wavefunctions. The method is applied to many-band problems.
		  Criteria are developed for the convergence of densities of
		  states, and the asymptotic convergence of the terminator is
		  related to singularities in the spectrum.}
}

