@Article{	  terakura84,
  author	= {K Masuda and N Hamada and K Terakura},
  title		= {Calculation of elastic constants of BCC transition metals:
		  tight-binding recursion method},
  journal	= {Journal of Physics F: Metal Physics},
  volume	= {14},
  number	= {1},
  pages		= {47},
  url		= {http://stacks.iop.org/0305-4608/14/i=1/a=007},
  year		= {1984},
  abstract	= {The elastic constants of BCC transition metals (Fe, Nb, Mo
		  and W) are calculated by using the tight-binding d band and
		  the Born-Mayer repulsive potential. Introducing a small
		  distortion characteristic to C 44 (or C') elastic
		  deformation and calculating the energy change up to second
		  order in the atomic displacement, the authors determine the
		  shear elastic constants C 44 and C'. The elastic constants
		  C 11 and C 12 are then calculated by using the relations B=
		  1 / 3 (C 11 +2C 12 ) and C'= 1 / 2 (C 11 -C 12 ), where B
		  is the bulk modulus. In general, the agreement between the
		  present results and the experimental values is
		  satisfactory. The characteristic elasticity behaviour, i.e.
		  the strong N d (number of d electrons) dependence of the
		  observed anisotropy factor A=C 44 /C', is also discussed.}
}

