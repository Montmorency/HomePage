@article{hammerschmidt19,
title = "BOPfox program for tight-binding and analytic bond-order potential calculations",
journal = "Computer Physics Communications",
volume = "235",
pages = "221 - 233",
year = "2019",
issn = "0010-4655",
doi = "https://doi.org/10.1016/j.cpc.2018.08.013",
url = "http://www.sciencedirect.com/science/article/pii/S0010465518303096",
author = "T. Hammerschmidt and B. Seiser and M.E. Ford and A.N. Ladines and S. Schreiber and N. Wang and J. Jenke and Y. Lysogorskiy and C. Teijeiro and M. Mrovec and M. Cak and E.R. Margine and D.G. Pettifor and R. Drautz",
keywords = "Atomistic simulations, Tight-binding approximation, Bond-order potentials",
abstract = "Bond-order potentials (BOPs) provide a local and physically transparent description of the interatomic interaction. Here we describe the efficient implementation of analytic BOPs in the BOPfox program and library. We discuss the integration of the underlying non-magnetic, collinear-magnetic and noncollinear-magnetic tight-binding models that are evaluated by the analytic BOPs. We summarise the flow of an analytic BOP calculation including the determination of self-returning paths for computing the moments, the self-consistency cycle, the estimation of the band-width from the recursion coefficients, and the termination of the BOP expansion. We discuss the implementation of the calculations of forces, stresses and magnetic torques with analytic BOPs. We show the scaling of analytic BOP calculations with the number of atoms and moments, present options for speeding up the calculations and outline different concepts of parallelisation. In the appendix we compile the implemented equations of the analytic BOP methodology and comments on the implementation. This description should be relevant for other implementations and further developments of analytic bond-order potentials."
}

