@article{godin91,
title = "The Block recursion library: accurate calculation of resolvent submatrices using the block recursion method",
journal = "Computer Physics Communications",
volume = "64",
number = "1",
pages = "123 - 130",
year = "1991",
issn = "0010-4655",
doi = "https://doi.org/10.1016/0010-4655(91)90055-P",
url = "http://www.sciencedirect.com/science/article/pii/001046559190055P",
author = "T.J. Godin and Roger Haydock",
keywords = "recursion method, resolvent, Green's functions, Schrödinger equation, transmittance",
abstract = "The Block Recursion Library, a collection of FORTRAN subroutines, calculates submatrices of the resolvent of a linear operator.
The resolvent, in matrix theory, is a powerful tool for extracting information about solutions of linear systems. The routines use the block
recursion method and achieve high accuracy for very large systems of coupled equations. This technique is a generalization of the scalar
recursion method, an accurate technique for finding the local density of states. A sample program uses these routines to find the quantum
mechanical transmittance of a randomly disordered two-dimensional cluster of atoms."
}

@article{nex89,
title = "The block Lanczos algorithm and the calculation of matrix resolvents",
journal = "Computer Physics Communications",
volume = "53",
number = "1",
pages = "141 - 146",
year = "1989",
issn = "0010-4655",
doi = "https://doi.org/10.1016/0010-4655(89)90155-0",
url = "http://www.sciencedirect.com/science/article/pii/0010465589901550",
author = "C.M.M. Nex",
abstract = "In solid state physics it is often required to calculate elements of the resolvent matrix ƒT(EI−H)−1g, where H is a large
Hermitian matrix, ƒ and g are column vectors and E is a scalar variable. When ƒ = g the Lanczos algorithm (in the context of the recursion
method) provides an effective means of solution, but for ƒ ≠ g some modifications are needed. In this paper we discuss the use of block
Lanczos iteration to block tridiagonalise H and examine used to calculate projections of the operator matrix form of orthogonal polynomials
and continued fractions. These are used to calculate projections of the operator (EI−H)−1) onto a subspace including ƒ and g (and any other
vectors of interest) and hence to estimate the matrix resolvent elements."
}

